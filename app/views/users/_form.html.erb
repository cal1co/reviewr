<%= form_with model: @user, local:true, html:{multipart:true} do |f| %>

    <%= f.file_field :image%>

    <%= f.label :bio, :class => 'form-label', class:'form-label' %>
    <%= f.text_field :bio, :maxlength => 140%>
    <%= f.date_field :birthday, value: f.object.try(:strftime,"%Y/%m/%d") %>

    <%= f.submit %>

<% end %>