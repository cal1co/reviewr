
<%= form_with url: login_path, local: true do |f| %>
<div class="container">
<div class="row">
<h2>LOGIN</h2>
</div>

<div class="row">
    <%= f.text_field :email, autocomplete:"off", placeholder:"email" %>
</div>
<div class="row">
    <%= f.password_field :password, placeholder:"password" %>
</div>
<div class="row">
    <%= f.submit 'LOGIN', class: "btn btn-outline-primary" %>
</div>
</div>
<% end %>